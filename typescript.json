{
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	},

	"Describe": {
		"prefix": "des",
		"body": [
			"describe('$1', ()=> {",
					"$2",
			"});"
		],
		"description": "describe a unit test"
	},

	"BeforeEachTest": {
		"prefix": "before",
		"body": [
		  "beforeEach(() => {",
			  "$1",
			"});"
		  ],
		"description": "before each test"
	},

	"individual test": {
		"prefix": "it",
		"body": [
		  "it('$1', () => {",
		     "$2",
		   "});"
		  ],
		"description": "each test"
	},

"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('${1:label');",
			"${1:label}"
		],
		"description": "Log output to console"
	}

}
